# -*- mode: snippet -*-
# name: sh
# key: sh
# --
#!/usr/bin/env bash
set -e

progname=\$0

function usage {
    echo "USAGE:"
    echo "    $progname [OPTIONS]"
    echo ""
    echo "ARGS:"
    echo "  -h, --help"
    echo "          Prints help information. Use --help for more details."
    exit 1
}

case \$1 in
    '-h' | '--help' )
        usage
        ;;
esac

